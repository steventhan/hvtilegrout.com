{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": 2,
        "quotes": ["error", "double"]
    },
    "env": {
        "browser": true,
        "node": true
    }
}
